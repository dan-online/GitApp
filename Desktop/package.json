{
  "name": "GitApp",
  "version": "3.3.1",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "release": "sudo rm -rf dist; nice -n2 sudo build -mwl -p always;",
    "release-patch": "npm version patch; npm run release;",
    "release-minor": "npm version minor; npm run release;",
    "release-major": "npm version major; npm run release;"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dan-online/GitApp"
  },
  "build": {
    "publish": [
      {
        "provider": "github",
        "owner": "dan-online",
        "repo": "GitApp",
        "publishAutoUpdate": true,
        "releaseType": "prerelease"
      }
    ],
    "dmg": {
      "background": "./build/icon.gif"
    },
    "artifactName": "${productName}-${os}-${version}.${ext}",
    "appId": "com.github.dan-online.GitApp"
  },
  "keywords": [
    "Electron app",
    "Github Web",
    "Github App"
  ],
  "author": "DanCodes",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^5.0.4"
  },
  "dependencies": {
    "custom-electron-titlebar": "^3.0.9",
    "electron-log": "^3.0.6",
    "electron-prompt": "^1.3.1",
    "electron-updater": "^4.0.6",
    "file-system": "^2.2.2",
    "is-online": "^8.2.0",
    "jquery": "^3.4.1"
  }
}
